{
  "name": "deCONZ",
  "version": "0.2.1",
  "slug": "deconz",
  "description": "deCONZ add-on",
  "startup": "services",
  "arch": [
    "aarch64",
    "armhf"
  ],
  "boot": "auto",
  "hassio_api": false,
  "homeassistant_api": false,
  "host_network": true,
  "webui": "http://[HOST]:[PORT:80]",
  "options": {
    "debug_info": "1",
    "debug_aps": "0",
    "debug_zcl": "0",
    "debug_zdp": "0",
    "debug_otau": "0",
    "web_port": "80",
    "websockets_port": "433"
  },
  "schema": {
    "debug_info": "int",
    "debug_aps": "int",
    "debug_zcl": "int",
    "debug_zdp": "int",
    "debug_otau": "int",
    "web_port": "int",
    "websockets_port": "int"
  },
  "devices": [
    "/dev/ttyUSB0:/dev/ttyUSB0:rwm"
  ]
}
